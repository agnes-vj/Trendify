<div class="columns-1 sm:columns-2 md:columns-3 lg:columns-4 w-full gap-2 space-y-4 p-4">
    @foreach (var item in clothesList)
    {
        <ClothingItem clothing="item"></ClothingItem>
    }
</div>

@code {

    [Parameter]
    public List<Backend.Models.ClothingItem> clothesList { get; set; }

}
